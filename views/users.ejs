<%- include("partials/_header") %>

   <body class="main-layout inner_page">
      <!-- loader  -->
      <div class="loader_bg">
         <div class="loader"><img src="images/loading.gif" alt="#" /></div>
      </div>
      <!-- end loader -->

      <!-- header -->
      <%- include("partials/_menu") %>
         <!-- end header inner -->
         <!-- team -->
         <div class="team">
            <div class="container_with">
               <div class="row">
                  <div class="col-md-12">
                     <div class="titlepage text_align_center">
                        <span>05</span>
                        <h2>Our Blog</h2>
                     </div>
                  </div>

                  <% for (user of users) { %>

                     <div class="col-md-4">
                        <div class="team_box text_align_center">
                           <div class="team_img">
                              <figure><img src="images/team1.jpg" alt="#" /></figure>
                           </div>
                           <div class="craig">
                              <h3>
                                 <%= user.username %>
                              </h3>
                              <span>PHOTOGRAPHER</span>
                              <ul class="social_icon ">
                                 <li><a href="Javascript:void(0)"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                                 </li>
                                 <li><a href="Javascript:void(0)"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                                 </li>
                                 <li><a href="/users/<%= user._id %>"><i class="fa fa-external-link-square"
                                          aria-hidden="true"></i></a>
                                 </li>
                              </ul>
                           </div>
                        </div>
                     </div>

                     <% } %>

               </div>
            </div>
         </div>
         <!-- end team -->
         <!-- end footer -->
         <%- include("partials/_footer") %>